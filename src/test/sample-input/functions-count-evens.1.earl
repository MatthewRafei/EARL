# Counts the number of even numbers in a range

fn count_evens(start, end) {
    let count = 0;

    for i in start..end {
        if i % 2 == 0 {
            count = count + 1;
        }
    }

    return count;
}

let start = 0;
let end = 10000000;
return count_evens(start, end);
