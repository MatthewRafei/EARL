# Counts the number of even numbers in a range

def count_evens(start: int, end: int) -> int {
    let count: int = 0;

    for i in start..end {
        if i % 2 == 0 {
            count = count + 1;
        }
    }

    return count;
}

let start: int = 0;
let end: int = 10000000;
return count_evens(start, end);