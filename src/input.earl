mod Main

let x = 99;

match x {
    1 when x % 2 == 0 -> {
        print("x is one and it is even???");
    }
    1 -> {
        print("x is one and it is odd");
    }
    2 -> {
        print("x is two");
    }
    3 | 4 -> {
        print("x is three or four");
    }
    _ -> {
        print("x is something else");
    }
}

print("done");
