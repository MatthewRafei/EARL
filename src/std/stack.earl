import "std/list.earl"
mod Stack

class T [init] {
    @ref lst = init;

    @pub
    fn push(x) {
        let test = List.range(1, 10, 1);
        lst.append(x);
    }

    @pub
    fn pop() {
        lst.pop(len(lst)-1);
    }

    @pub
    fn empty() {
        return len(lst) == 0;
    }

    @pub
    fn top() {
        return lst[len(lst)-1];
    }
}
