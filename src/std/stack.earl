mod Stack

@pub
class T [init] {
    @ref lst = init;

    @pub
    fn push(x) {
        this.lst.append(x);
    }

    @pub
    fn pop() {
        this.lst.pop(len(this.lst)-1);
    }

    @pub
    fn empty() {
        return len(this.lst) == 0;
    }

    @pub
    fn top() {
        return this.lst[len(this.lst)-1];
    }
}
