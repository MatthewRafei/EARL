mod Std

@pub
fn is_even(n) {
    return n%2 == 0;
}

@pub
fn swap(@ref x1, @ref x2) {
    print("swapping: ", x1, " ", x2);
    let t = x1;
    x1 = x2;
    x2 = t;
}

@pub
fn contains(@ref lst, k) {
    for i in 0..len(lst) {
        if lst[i] == k {
            return 1;
        }
    }
    return 0;
}
