module Main

import "std/io.earl"
import "lexer.earl"

let comment, multicom_st, multicom_end = ("#", "/*", "*/");
let keywords = ["int", "void"];
let ops = Dict(str);

let fp = "./std-docs-gen.earl";
let src = IO::file_to_str(fp);

let lexer = Lexer::T(keywords, comment, multicom_st, multicom_end, ops);

lexer.lex(src, fp);
println(lexer);


