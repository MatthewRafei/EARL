import "std/std.earl"

mod Main

fn rec(i) {
    if i == 3 {
        return 1;
    }
    return rec(i+1);
}

fn fib(n) {
    if n == 0 || n == 1 {
        return n;
    }
    return fib(n-1) + fib(n-2);
}

fn helper(a) { return a*2; }

fn test(i) {
    return helper(i) + helper(i);
}

print(test(3));
print(rec(0));
print(fib(10));
