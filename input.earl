fn test_list_append_intrinsic2() {
    print("test_list_append_intrinsic2...");

    let lst = [1,2,3];
    lst.append(4, "foobar", [5,6,7]);

    let t = 1;

    for i in 0..len(lst)-2 {
        assert(lst.nth(i) == t);
        t = t+1;
    }

    assert(lst.nth(4) == "foobar");
    assert(lst.nth(5) == [5,6,7]);

    print("ok");
}


test_list_append_intrinsic2();
